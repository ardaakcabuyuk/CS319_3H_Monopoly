<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ApplicationControl.CafePopupController">
    <children>
        <VBox layoutX="182.0" layoutY="12.0" prefHeight="369.0" prefWidth="208.0" spacing="30.0" AnchorPane.bottomAnchor="19.0" AnchorPane.leftAnchor="182.0" AnchorPane.rightAnchor="210.0" AnchorPane.topAnchor="12.0">
            <children>
                <Label fx:id="cafeName" alignment="CENTER" contentDisplay="CENTER" prefHeight="28.0" prefWidth="215.0" text="Label Name" />
                <ImageView fx:id="landImage" fitHeight="138.0" fitWidth="253.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@Images/irem.jpeg" />
                    </image>
                    <VBox.margin>
                        <Insets left="35.0" />
                    </VBox.margin></ImageView>
                <VBox prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Label fx:id="rentWith1" alignment="CENTER" layoutX="10.0" layoutY="32.0" prefHeight="19.0" prefWidth="228.0" text="1 Cafe" />
                        <Label fx:id="rentWith2" alignment="CENTER" layoutX="10.0" layoutY="32.0" prefHeight="20.0" prefWidth="226.0" text="2 Cafe" />
                        <Label fx:id="rentWith3" alignment="CENTER" layoutX="10.0" layoutY="32.0" prefHeight="21.0" prefWidth="227.0" text="3 Cafe" />
                        <Label fx:id="rentWith4" alignment="CENTER" layoutX="10.0" layoutY="32.0" prefHeight="21.0" prefWidth="227.0" text="4 Cafe" />
                    </children>
                </VBox>
                  <Button onAction="#buyButtonClicked" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="25.0" prefWidth="74.0" style="-fx-background-radius: 30; -fx-background-color: aquamarine;" text="Buy" fx:id="buyButton">
               <VBox.margin>
                  <Insets left="74.0" />
               </VBox.margin>
                  </Button>
            </children>
        </VBox>
        <Button layoutX="534.0" layoutY="12.0" mnemonicParsing="false" onAction="#closeButtonClicked" style="-fx-background-color: white;">
            <graphic>
                <ImageView fitHeight="42.0" fitWidth="79.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@Images/close_icon.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</AnchorPane>
