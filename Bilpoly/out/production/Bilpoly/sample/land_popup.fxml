<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LandPopupController">
   <children>
      <VBox layoutX="182.0" layoutY="12.0" prefHeight="369.0" prefWidth="208.0" spacing="30.0" AnchorPane.bottomAnchor="19.0" AnchorPane.leftAnchor="182.0" AnchorPane.rightAnchor="210.0" AnchorPane.topAnchor="12.0">
         <children>
            <Label fx:id="landName" alignment="CENTER" contentDisplay="CENTER" prefHeight="28.0" prefWidth="215.0" text="Label Name" />
            <ImageView id="landImage" fx:id="landImage" fitHeight="138.0" fitWidth="253.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/ece.jpeg" />
               </image>
               <VBox.margin>
                  <Insets left="35.0" />
               </VBox.margin></ImageView>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label fx:id="rent" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="226.0" text="Rent" />
                  <Label fx:id="rentWithColorSet" alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="20.0" prefWidth="224.0" text="Rent with color set" />
                  <Label fx:id="rentWithOneSB" alignment="CENTER" layoutX="10.0" layoutY="32.0" prefHeight="19.0" prefWidth="228.0" text="1 Starbucks" />
                  <Label fx:id="rentWithTwoSB" alignment="CENTER" layoutX="10.0" layoutY="32.0" prefHeight="20.0" prefWidth="226.0" text="2 Starbucks" />
                  <Label fx:id="rentWithThreeSB" alignment="CENTER" layoutX="10.0" layoutY="32.0" prefHeight="21.0" prefWidth="227.0" text="3 Starbucks" />
                  <Label fx:id="rentWithBilka" alignment="CENTER" layoutX="10.0" layoutY="32.0" prefHeight="21.0" prefWidth="227.0" text="Bilka" />
                  <Label alignment="CENTER" layoutX="10.0" layoutY="32.0" prefHeight="20.0" prefWidth="224.0" text="Build Starbucks" />
                  <Label alignment="CENTER" layoutX="10.0" layoutY="32.0" prefHeight="25.0" prefWidth="227.0" text="Build Bilka" />
               </children>
            </VBox>
      
            <Button fx:id="buyButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#buyButtonClicked" prefHeight="25.0" prefWidth="74.0" style="-fx-background-radius: 30; -fx-background-color: aquamarine;" text="Buy">
               <VBox.margin>
                  <Insets left="74.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <Button fx:id="closeButton" layoutX="534.0" layoutY="12.0" mnemonicParsing="false" onAction="#closeButtonClicked" style="-fx-background-color: white;">
         <graphic>
            <ImageView fitHeight="42.0" fitWidth="79.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Images/close_icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
